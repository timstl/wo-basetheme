{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": [
				{
					"slug": "primary",
					"color": "var(--bs-primary)",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "var(--bs-secondary)",
					"name": "Secondary"
				},
				{
					"slug": "success",
					"color": "var(--bs-success)",
					"name": "Success"
				},
				{
					"slug": "info",
					"color": "var(--bs-info)",
					"name": "Info"
				},
				{
					"slug": "warning",
					"color": "var(--bs-warning)",
					"name": "Warning"
				},
				{
					"slug": "danger",
					"color": "var(--bs-danger)",
					"name": "Danger"
				},
				{
					"slug": "gray-400",
					"color": "var(--bs-gray-400)",
					"name": "Gray-400"
				},
				{
					"slug": "gray-300",
					"color": "var(--bs-gray-300)",
					"name": "Gray-300"
				},
				{
					"slug": "gray-200",
					"color": "var(--bs-gray-200)",
					"name": "Gray-200"
				},
				{
					"slug": "gray-100",
					"color": "var(--bs-gray-100)",
					"name": "Gray-200"
				},
				{
					"slug": "white",
					"color": "var(--bs-white)",
					"name": "White"
				},
				{
					"slug": "body-color",
					"color": "var(--bs-body-color)",
					"name": "Body Color"
				}
			],
			"gradients": []
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "var(--bs-font-sans-serif)",
					"slug": "sans-serif",
					"name": "Sans Serif"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "var(--bs-font-size-sm)",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "var(--bs-font-size-md)",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "var(--bs-font-size-lg)",
					"name": "Large"
				},
				{
					"slug": "extra-large",
					"size": "var(--bs-font-size-xl)",
					"name": "Extra large"
				}
			]
		},
		"layout": {
			"contentSize": "990px",
			"wideSize": "1400px"
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"units": ["px", "em", "rem", "vh", "vw", "%"]
		}
	}
}
