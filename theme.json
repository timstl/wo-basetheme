{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"palette": [
				{
					"slug": "primary",
					"color": "var(--bs-primary)",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "var(--bs-secondary)",
					"name": "Secondary"
				},
				{
					"slug": "success",
					"color": "var(--bs-success)",
					"name": "Success"
				},
				{
					"slug": "gray-200",
					"color": "var(--bs-gray-200)",
					"name": "Gray-200"
				},
				{
					"slug": "gray-100",
					"color": "var(--bs-gray-100)",
					"name": "Gray-200"
				},
				{
					"slug": "white",
					"color": "var(--bs-white)",
					"name": "White"
				},
				{
					"slug": "body-color",
					"color": "var(--bs-body-color)",
					"name": "Body Color"
				}
			],
			"gradients": []
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto,\"Helvetica Neue\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\",\"Segoe UI Symbol\"",
					"slug": "sans-serif",
					"name": "Sans Serif"
				},
				{
					"fontFamily": "serif",
					"slug": "serif",
					"name": "Serif"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.75rem",
					"name": "Small"
				},
				{
					"slug": "base",
					"size": "1rem",
					"name": "Normal"
				},
				{
					"slug": "medium",
					"size": "1.25rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "2.25rem",
					"name": "Large"
				},
				{
					"slug": "extra-large",
					"size": "3rem",
					"name": "Extra large"
				}
			]
		},
		"layout": {
			"contentSize": "990px",
			"wideSize": "1400px"
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"units": ["px", "em", "rem", "vh", "vw", "%"]
		}
	}
}
